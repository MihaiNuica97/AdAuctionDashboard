<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="301.0" prefWidth="504.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="segGroupCW.FileLoadController">
    <children>
        <Label alignment="TOP_CENTER" text="Upload your Files" />
      <FlowPane alignment="TOP_CENTER" prefHeight="37.0" prefWidth="739.0">
         <children>
             <TextField fx:id="clicksPathLabel" promptText="click_log.csv" />
             <Button fx:id="browseClicksBtn" alignment="CENTER" text="Browse..." />
             <Label text="   click_log.csv" />
         </children>
      </FlowPane>

      <FlowPane alignment="TOP_CENTER" prefHeight="25.0" prefWidth="0.0">
          <TextField fx:id="imprPathLabel" promptText="impression_log.csv" />
          <Button fx:id="browseImprBtn" alignment="CENTER" text="Browse..." />
          <Label text="   impression_log.csv" />
      </FlowPane>

      <FlowPane alignment="TOP_CENTER" prefHeight="35.0" prefWidth="464.0">
          <TextField fx:id="serverPathLabel" promptText="server_log.csv" />
          <Button fx:id="browseServerBtn" alignment="CENTER" text="Browse..." />
          <Label text="   server_log.csv" />
      </FlowPane>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="dashboardBtn" onAction="#goToDashboard" alignment="CENTER" contentDisplay="CENTER" text="Go to Dashboard" />
         </children>
      </FlowPane>


    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
